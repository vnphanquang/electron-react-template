{
  "name": "etrt",
  "version": "0.0.1",
  "description": "Electron in Typescript with React & TailwindCSS",
  "scripts": {
    "build:main:prod": "cross-env NODE_ENV=production webpack --config ./.config/webpack.main.js",
    "build:main:dev": "cross-env NODE_ENV=development webpack --config ./.config/webpack.main.js",
    "build:renderer:prod": "cross-env NODE_ENV=production snowpack build",
    "build:renderer:dev": "cross-env NODE_ENV=development snowpack build",
    "dev:renderer": "snowpack dev",
    "dev:main": "yarn build:main:dev && electron build/main.js",
    "dev": "run-p dev:renderer && dev:main"
  },
  "keywords": [],
  "author": "Quang Phan",
  "license": "MIT",
  "dependencies": {
    "deepmerge": "^4.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@snowpack/plugin-babel": "^2.1.5",
    "@snowpack/plugin-postcss": "^1.0.11",
    "@snowpack/plugin-react-refresh": "^2.3.9",
    "@snowpack/plugin-webpack": "^2.2.0",
    "@tailwindcss/typography": "^0.3.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "electron": "^11.1.1",
    "eslint": "^7.16.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "snowpack": "^2.18.5",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-merge": "^5.7.3"
  }
}
